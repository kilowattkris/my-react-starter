{
    "name": "my-react-starter",
    "version": "0.1.0",
    "description": "The starter kit I like to use",
    "author": "Kristine Tomlinson",
    "repository": {
        "type": "git",
        "url": "https://kristinetomlinson@bitbucket.org/lingomedia/ell_lms_fe.git"
    },
    "babel": {
        "presets": [
            "es2015",
            "stage-2",
            "react"
        ],
        "env": {
            "production": {
                "plugins": [
                    "transform-react-remove-prop-types",
                    "transform-react-constant-elements"
                ]
            }
        }
    },
    "scripts": {
        "startMsg": "babel-node buildScripts/startMessage.js",
        "prestart": "npm-run-all startMsg",
        "start": "npm-run-all --parallel serve lint:watch",
        "serve": "babel-node buildScripts/srcServer.js",
        "lint": "node_modules/.bin/esw webpack.config.* src buildScripts",
        "lint:watch": "npm run lint -- --watch",
        "test": "jest",
        "test:watch": "npm run test -- --watch",
        "prebuild": "npm-run-all dist:clean lint",
        "build": "npm-run-all dist:build",
        "postbuild": "babel-node buildScripts/distServer.js",
        "dist:clean": "rimraf ./dist && mkdir dist",
        "dist:build": "babel-node buildScripts/build.js"
    },
    "private": true,
    "dependencies": {
        "axios": "^0.18.0",
        "babel-polyfill": "^6.26.0",
        "bootstrap": "^4.0.0",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-redux": "5.0.6",
        "react-router-dom": "^4.2.2",
        "react-router-redux": "4.0.8",
        "redux": "3.7.2",
        "redux-thunk": "^2.2.0",
        "styled-components": "^3.2.1",
        "toastr": "^2.1.4"
    },
    "devDependencies": {
        "autoprefixer": "7.1.2",
        "babel-cli": "6.26.0",
        "babel-core": "6.26.0",
        "babel-eslint": "7.2.3",
        "babel-loader": "6.4.1",
        "babel-plugin-dynamic-import-webpack": "1.0.2",
        "babel-plugin-remove-webpack": "1.1.0",
        "babel-plugin-transform-react-constant-elements": "6.23.0",
        "babel-plugin-transform-react-remove-prop-types": "0.4.8",
        "babel-preset-es2015": "6.24.1",
        "babel-preset-react": "6.24.1",
        "babel-preset-stage-2": "6.24.1",
        "babel-runtime": "6.26.0",
        "chalk": "1.1.3",
        "compression": "1.7.0",
        "cross-env": "5.0.5",
        "css-loader": "0.28.5",
        "ejs": "2.5.7",
        "eslint": "3.19.0",
        "eslint-plugin-import": "2.7.0",
        "eslint-plugin-react": "7.2.1",
        "eslint-watch": "3.1.2",
        "eventsource-polyfill": "0.9.6",
        "express": "4.15.4",
        "extract-text-webpack-plugin": "2.1.2",
        "file-loader": "0.11.2",
        "html-webpack-plugin": "2.30.1",
        "jest": "^22.4.2",
        "node-sass": "4.0.0",
        "npm-run-all": "4.0.2",
        "open": "0.0.5",
        "postcss-loader": "2.0.5",
        "prop-types": "15.5.10",
        "react-addons-test-utils": "15.6.0",
        "react-code-splitting": "1.2.1",
        "react-test-renderer": "15.6.1",
        "redux-immutable-state-invariant": "2.0.0",
        "redux-mock-store": "1.2.3",
        "rimraf": "2.5.2",
        "sass-loader": "6.0.6",
        "strip-loader": "0.1.2",
        "style-loader": "0.13.2",
        "transfer-webpack-plugin": "0.1.4",
        "uglifyjs-webpack-plugin": "1.1.2",
        "url-loader": "0.5.9",
        "warning": "3.0.0",
        "webpack": "2.2.1",
        "webpack-bundle-analyzer": "2.1.1",
        "webpack-dev-middleware": "1.9.0",
        "webpack-hot-middleware": "2.13.2"
    }
}